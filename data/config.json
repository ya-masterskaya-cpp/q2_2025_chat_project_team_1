{
  "listeners": [
    {
      "address": "127.0.0.1",
      "port": 8080
    }
  ],
  "log": {
    "log_level": "ERROR",
    "log_path": "./logs",
    "log_file": "server.log"
  },
  "plugins": [
    {
      "name": "LoggerPlugin",
      "type": "Global"
    },
    {
      "name": "DatabasePlugin",
      "config": {
        "conn_str": "host=localhost dbname=chat_db user=chat_user password=chat_pass",
        "pool_size": 4
      }
    },
    {
      "name": "ChatServicePlugin",
      "type": "Global"
    }
  ]
}